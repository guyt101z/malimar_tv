$('#grid_<%= @grid.id %>').html('<%= escape_javascript(render("videos/partials/grid")).html_safe %>')


$("#grid_<%= @grid.id %>").slick({
            infinite: true,
            slidesToShow: 6,
            slidesToScroll: 3,
            responsive: [
                {
                  breakpoint: 991,
                  settings: {
                    slidesToShow: 4,
                    slidesToScroll: 2,
                    infinite: true
                  }
                },
                {
                  breakpoint: 768,
                  settings: {
                    slidesToShow: 3,
                    slidesToScroll: 1
                  }
                },
                {
                  breakpoint: 480,
                  settings: {
                    slidesToShow: 3,
                    slidesToScroll: 1
                  }
                }
            ]
        });