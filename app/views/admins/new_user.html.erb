<% content_for :top_level, 'Clients' %>
<% content_for :low_levels, YAML.dump(['New Client']) %>
<%= form_tag admin_create_user_path, remote: true do %>
<div id="left-scroll" class="nano">
    <div class="nano-content">

        <div class="panel">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-sm-6">
                        <span class="icon-user-add mr7"></span> Register Client
                    </div>
                </div>
            </div>
            <div class="panel-body plr20">
                <%= render 'admins/new_user/user_form' %>
                <div class="line"></div>
                <%= render 'admins/new_user/device_form' %>
                <div class="line"></div>
                <%= render 'admins/new_user/tx_form' %>
                <div class="line"></div>
                <%= render 'admins/new_user/note_form' %>
                <div class="line"></div>
                <div class="text-right">
					<%= submit_tag 'Register Client', class: 'btn btn-primary btn-w170 btn-sm' %>
                </div>
            </div>
        </div>

	</div>
	</div>
<% end %>
