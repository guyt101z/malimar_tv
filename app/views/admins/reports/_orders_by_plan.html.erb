<script>
$(document).ready(function(){
    var chart2 = Morris.Bar({
        element: 'reports_2',
        data: [
        <% @plans.each do |k, plan| %>
          { y: '<%= plan.name %>', a: <%= @plan_totals[k] %> },
        <% end %>
        ],
        xkey: 'y',
        ykeys: ['a'],
        labels: ['Transactions'],
        resize: true,
        barColors: ['#73DEA9'],
        hideHover: true
    });
})
$(document).on('page:load', function(){
    var chart2 = Morris.Bar({
        element: 'reports_2',
        data: [
        <% @plans.each do |k, plan| %>
          { y: '<%= plan.name %>', a: <%= @plan_totals[k] %> },
        <% end %>
        ],
        xkey: 'y',
        ykeys: ['a'],
        labels: ['Transactions'],
        resize: true,
        barColors: ['#73DEA9'],
        hideHover: true
    });
})
</script>
