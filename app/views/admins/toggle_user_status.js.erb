<% if @user.is_active? %>
    $('#client-status').html('<%= link_to "Client Enabled", admin_toggle_user_status_path(id: @user.id), class: "btn btn-block btn-success", data: {disable_with: "Disabling..."}, remote: true %>')
<% else %>
    $('#client-status').html('<%= link_to "Client Disabled", admin_toggle_user_status_path(id: @user.id), class: "btn btn-block btn-warning", data: {disable_with: "Enabling..."}, remote: true %>')
<% end %>
