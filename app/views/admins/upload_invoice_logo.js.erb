<% if @success == true %>
    $('#invoice_image').attr('src','<%= @logo.image_url %>')
    addNotification('success', 'Your logo has been successfully uploaded', 'Success', 'check-circle')
<% else %>
    addNotification('error', 'There was an error uploading the image, please ensure that the file is either a jpg/jpeg, png, or gif', 'Error', 'exclamation-circle')
<% end %>
