<div id="left-scroll" class="nano">
    <div class="nano-content">
        <% @grids.each do |grid| %>
            <% @grid = grid %>
            <%= render 'admins/cms_partials/grid_details' %>
        <% end %>
    </div>
</div>
<%= render 'admins/cms_partials/new_grid' %>
<%= render 'admins/cms_partials/edit_grid' %>
<script>
$('#home_grid').addClass('active')
</script>
